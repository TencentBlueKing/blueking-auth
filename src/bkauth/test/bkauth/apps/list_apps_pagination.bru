meta {
  name: list_apps_pagination
  type: http
  seq: 11
}

get {
  url: http://{{host}}:{{port}}/api/v1/apps?page=2&page_size=2
  body: json
  auth: none
}

params:query {
  page: 2
  page_size: 2
}

headers {
  X-Bk-App-Code: {{x_bk_app_code}}
  X-Bk-App-Secret: {{x_bk_app_secret}}
}

assert {
  res.status: eq 200
  res.body.message: eq ok
  res.body.data.count: eq 3
  res.body.data.results: length 1
}
