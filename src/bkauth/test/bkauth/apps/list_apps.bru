meta {
  name: list_apps
  type: http
  seq: 10
}

get {
  url: http://{{host}}:{{port}}/api/v1/apps
  body: json
  auth: none
}

headers {
  X-Bk-App-Code: {{x_bk_app_code}}
  X-Bk-App-Secret: {{x_bk_app_secret}}
}

assert {
  res.status: eq 200
  res.body.message: eq ok
  res.body.data.count: eq 3
  res.body.data.results: length 3
}
