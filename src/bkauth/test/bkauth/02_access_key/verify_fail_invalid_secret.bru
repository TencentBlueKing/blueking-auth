meta {
  name: verify_fail_invalid_secret
  type: http
  seq: 29
}

post {
  url: http://{{host}}:{{port}}/api/v1/apps/demo/access-keys/verify
  body: json
  auth: none
}

headers {
  X-Bk-App-Code: {{x_bk_app_code}}
  X-Bk-App-Secret: {{x_bk_app_secret}}
}

body:json {
  {
    "bk_app_secret": "invalid_secret_123"
  }
}

assert {
  res.status: eq 200
  res.body.code: eq 0
  res.body.message: eq bk_app_code or bk_app_secret invalid
  res.body.data.is_match: eq false
}