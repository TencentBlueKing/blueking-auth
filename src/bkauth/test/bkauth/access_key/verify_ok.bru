meta {
  name: verify_ok
  type: http
  seq: 4
}

post {
  url: http://{{host}}:{{port}}/api/v1/apps/demo/access-keys/verify
  body: json
  auth: none
}

headers {
  X-Bk-App-Code: {{x_bk_app_code}}
  X-Bk-App-Secret: {{x_bk_app_secret}}
}

body:json {
  {
    "bk_app_secret": "{{app_secret}}"
  }
}

assert {
  res.status: eq 200
  res.body.code: eq 0
  res.body.message: eq ok
  res.body.data.is_match: eq true
}